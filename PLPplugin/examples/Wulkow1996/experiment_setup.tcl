# time tick
set DT 0.01
# Burst delay
set BD 0
# Bursts number
set BN 1
# Number of laser pulses
set NP 16
# Pulses repetition rate (Hz)
set PRR [expr 1/0.4]
# Total simulation time (s)
set ST 7
# Simulation Number of Molecules
set SNM 1e14


# Model settings
# primary radicals conc. per pulse
set R0 1e-7

set MW 1
set M0 9
set kip 120
set kp 120
set ktd 3e7
set ktc 0

